<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
     http://www.springframework.org/schema/beans/spring-beans.xsd
     http://www.springframework.org/schema/context
     http://www.springframework.org/schema/context/spring-context.xsd">

    <context:annotation-config />

    <bean id="employeeId" class="_01_AutoWiring.Employee1">
        <property name="name" value="Murat"/>
        <property name="surname" value="Boz"/>
        <property name="salary" value="23000"/>
    </bean>

    <bean id="adressId" class="_01_AutoWiring.Adress">
        <property name="city" value="İstanbul"/>
    </bean>

    <bean id="adressId1" class="_01_AutoWiring.Adress">
        <property name="city" value="Ankara"/>
    </bean>

    <bean id="adressId2" class="_01_AutoWiring.Adress">
        <property name="city" value="İzmir"/>
    </bean>

    <bean id="departmentId" class="_01_AutoWiring.Department">
        <property name="department_Name" value="IT"/>
    </bean>
</beans>