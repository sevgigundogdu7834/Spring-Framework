<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="employeeId" class="tekrar.setter_injection.Employee">
        <property name="id" value="1"/>
        <property name="name" value="Sevgi"/>
        <property name="surname" value="Gündoğdu"/>
        <property name="salary" value="23000"/>
    </bean>

    <bean id="employeeId2" class="tekrar.setter_injection.Employee"


          p:id="2"
          p:name="Zühra"
          p:surname="Çelen"
          p:salary="3400"
    />



</beans>